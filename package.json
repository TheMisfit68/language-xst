{
    "repository": "https://github.com/TheMisfit68/language-xst.git",
    "publisher": "TheMisfit68",
    "name": "xst",
    "displayName": "xst",
    "icon": "images/icon.png",
    "description": "Syntax Coloring for Schneider Unity XST",
    "version": "1.0.1",
    "engines": {
        "vscode": "^1.69.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "Programming Languages",
        "Schneider Unity",
        "xst"
    ],
    "contributes": {
        "languages": [
            {
                "id": "xst",
                "aliases": [
                    "Schneider Unity XST",
                    "xst"
                ],
                "extensions": [
                    ".xst"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "xst",
                "scopeName": "source.xst",
                "path": "./syntaxes/xst.tmLanguage.json"
            },
            {
                "scopeName": "source.xml",
                "path": "./syntaxes/xml.tmLanguage.json"
            },
            {
                "scopeName": "source.st",
                "path": "./syntaxes/st.tmLanguage.json"
            }
        ]
    }
}
